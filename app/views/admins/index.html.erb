<section class="admin-table">
  <table class="admin-content-table">
    <Caption>All Users</Caption>

    <thead>
      <tr>
        <th>Email</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Username</th>
        <th>Action</th>
      </tr>
    </thead>

    <tbody>
      <% @users.each do |user|%>
        <tr>
          <td><%= user.email%></td>
          <td><%= user.first_name%></td>
          <td><%= user.last_name%></td>
          <td><%= user.username%></td>
          <%if user.type == 'Broker'%>
          <td><%= link_to 'Edit', edit_admin_broker_path(current_admin, user)%></td>
          <%end%>
        </tr>
      <%end%>
    </tbody>
  </table>
</section>